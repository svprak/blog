<h1 align ="center">List All Categories</h1>
<div align="center">
  <%= will_paginate %>
<% @categories.each do |c| %>
  <ul class="listing">
    <div class="row">
      <div class="well col-md-4 col-md-offset-4">
        <li> <%= link_to "#{c.name}", category_path(c)  %> </li>
      </div>
    </div>
  </ul>
<% end %>
<%= will_paginate %>
</div>
